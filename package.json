{
  "name": "projeto-typescript",
  "version": "1.0.0",
  "description": "Um projeto para estudo de Typescript",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "start:local": "ts-node-dev --transpile-only src/index.ts",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:unit": "jest",
    "test:functional": "jest --projects ./test",
    "style:check": "prettier --check src/**/*.ts test/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts test/**/*.ts"
  },
  "keywords": [
    "typescript",
    "javascript"
  ],
  "author": "Gustavo Borges",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.4.0",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.14.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "@types/express": "^4.17.17",
    "axios": "^1.3.3",
    "express": "^4.18.2",
    "module-alias": "^2.2.2"
  }
}
